{
  "tabnine.experimentalAutoImports": true,
  "git.autofetch": true,
  "editor.fontSize": 16,
  "debug.console.fontSize": 15,
  "terminal.integrated.fontSize": 16,
  "editor.suggestSelection": "first",
  "workbench.iconTheme": "material-icon-theme",
  "files.exclude": {
    "": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.git": false,
    "**/.project": true,
    "**/.settings": true
